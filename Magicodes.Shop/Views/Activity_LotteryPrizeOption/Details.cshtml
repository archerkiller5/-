@model Magicodes.WeiChat.Data.Models.Activity.Activity_LotteryPrizeOption

@{
    ViewBag.Title = "查看";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-9">
        <h2>@ViewBag.Title</h2>
        <ol class="breadcrumb">
            <li>
                <a href="/">主页</a>
            </li>
            <li>
                <a href="@Url.Action("Index")">上级页面</a>
            </li>
            <li class="active">
                <strong>@ViewBag.Title</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <hr style="margin:2px 2px 10px 2px;" />
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.CreateUser)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.CreateUser.UserName)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.UpdateUser)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.UpdateUser.UserName)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Title)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Title)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Prize)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Prize)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.PrizeCount)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PrizeCount)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.OverCount)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.OverCount)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.CreateTime)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.CreateTime)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.UpdateTime)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.UpdateTime)
                        </dd>

                    </dl>
                    <p>
                        @Html.ActionLink("编辑", "Edit", new { id = Model.Id }) |
                        @Html.ActionLink("返回", "Index",new { lotteryId = Request.QueryString["lotteryId"] })
                    </p>
                </div>

            </div>
        </div>
    </div>
</div>
@section Styles {
    @Styles.Render("~/Content/plugins/iCheck/iCheckStyles")
}
@section Scripts {
    @Scripts.Render("~/plugins/iCheck")
    <script type="text/javascript">
        $(function () {
            $("input[type=checkbox]").iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
        });
    </script>
}
